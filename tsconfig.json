{
    "compilerOptions": {
        "strict": true,
        "target": "es2022",
        "noImplicitAny": false,
        "noEmitOnError": true,
        "outDir": "out",
        "moduleResolution": "node",
        "useUnknownInCatchVariables": true,
        "allowUnreachableCode": false,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noPropertyAccessFromIndexSignature": false,
        "module": "commonjs",
        "sourceMap": true,
    },
    "include" : ["./settings.json", "src/**/*.ts"],
    "exclude": ["src/commands/messageCommands", "src/classes", "src/commands/slashCommands/IGNORE", "src/events/IGNORE", "src/shard.ts", "src/database"]
}